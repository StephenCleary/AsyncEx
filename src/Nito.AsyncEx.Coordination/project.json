{
  "version": "5.0.0-pre-01",
  "description": "Asynchronous coordination primitives.",
  "authors": [ "Stephen Cleary" ],
  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true
  },
  "packOptions": {
    "tags": [ "async", "asynclock", "asynclazy" ],
    "projectUrl": "https://github.com/StephenCleary/AsyncEx.Coordination",
    "licenseUrl": "https://raw.githubusercontent.com/StephenCleary/AsyncEx.Coordination/master/LICENSE",
    "repository": {
      "type": "git",
      "url": "https://github.com/StephenCleary/AsyncEx.Coordination.git"
    }
  },
  "frameworks": {
    "netstandard1.3": {
      "dependencies": {
        "System.Runtime": "4.1.0",
        "System.Threading": "4.0.11",
        "System.Threading.Tasks": "4.0.11"
      }
    },
    "net46": {
      "frameworkAssemblies": {
        "System.Runtime": { "type": "build" },
        "System.Threading": { "type": "build" },
        "System.Threading.Tasks": { "type": "build" }
      }
    }
  },
  "dependencies": {
    "Nito.Collections.Deque": "1.0.0",
	"Nito.Disposables": "1.1.0-alpha-02",
    "Nito.AsyncEx.Tasks": "5.0.0-pre-01"
  }
